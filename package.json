{
	"name": "obsidian-entities",
	"version": "0.3.7",
	"description": "Adds autocomplete of \"Entities\" like files or templates in a specific folder or a from a dataview query.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node scripts/update-versions.mjs $npm_package_version && git add manifest.json versions.json",
		"test": "jest",
		"release:pr": "release-please release-pr --token=$GITHUB_TOKEN --repo-url=https://github.com/gtg922r/obsidian-entities",
		"release:create": "release-please github-release --token=$GITHUB_TOKEN --repo-url=https://github.com/gtg922r/obsidian-entities",
		"release:beta": "node scripts/create-beta-release.mjs"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^16.18.104",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"moment": "^2.30.1",
		"obsidian": "^1.5.7-1",
		"obsidian-dataview": "^0.5.64",
		"release-please": "^17.1.1",
		"rollup": "^4.14.2",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"emojilib": "^3.0.12"
	}
}
